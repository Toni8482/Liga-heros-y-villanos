{
  "name": "liga-heroes-y-villanos",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start:misiones": "json-server --watch ./model_backend/misiones.json --port 3001",
